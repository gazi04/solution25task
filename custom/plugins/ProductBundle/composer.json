{
    "name": "product-bundle/product-bundle",
    "description": "product-bundle/product-bundle",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "shopware/core": "~6.7.0"
    },
    "extra": {
        "shopware-plugin-class": "ProductBundle\\ProductBundle",
        "label": {
            "de-DE": "Product bundle plugin",
            "en-GB": "Product bundle plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "ProductBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProductBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
